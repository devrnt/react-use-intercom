{
  "name": "react-use-intercom",
  "author": "devrnt",
  "module": "dist/react-use-intercom.esm.js",
  "description": "React Intercom integration without the hassle, driven by hooks.",
  "homepage": "https://github.com/devrnt/react-use-intercom#readme",
  "version": "1.1.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/devrnt/react-use-intercom"
  },
  "bugs": {
    "url": "https://github.com/devrnt/react-use-intercom/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/react-use-intercom.esm.js"
    }
  },
  "keywords": [
    "react",
    "hooks",
    "reactjs",
    "intercom",
    "intercomjs",
    "react-intercom",
    "intercom-react",
    "typescript",
    "react-hooks",
    "react-use-intercom"
  ],
  "scripts": {
    "start": "tsdx watch",
    "start:example": "yarn --cwd ./example install && yarn --cwd ./example start",
    "build": "tsdx build",
    "build:example": "yarn --cwd ./example install && yarn --cwd ./example build",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "yarn build && size-limit",
    "analyze": "yarn build && size-limit --why",
    "release": "np"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/react-use-intercom.cjs.production.min.js",
      "limit": "3 KB"
    },
    {
      "path": "dist/react-use-intercom.esm.js",
      "limit": "3 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.7.0",
    "@testing-library/react": "^11.0.0",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jsdom": "^16.2.3",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "cypress": "5.6.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "jsdom": "^16.3.0",
    "np": "^7.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-test-renderer": "^17.0.0",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.2"
  },
  "resolutions": {
    "**/**/yargs-parser": "^13.1.2"
  }
}
